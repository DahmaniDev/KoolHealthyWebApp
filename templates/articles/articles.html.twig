							<h4>{{recette3.titre}}</h4>
							<p>{{recette3.heading}}</p>
							<a class="btn btn-lg btn-circle btn-outline-new-white" style="position: absolute; bottom: 0; margin-bottom: 20px;" href="/recette/{{recette3.getId}}">Voir plus</a>
						</div>
					</div>
